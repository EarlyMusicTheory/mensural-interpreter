{
    "plugins": ["plugins/markdown"],
    "recurseDepth": 10,
    "source": {
        "include": ["./js/"],
        "exclude": []
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    },
    "opts": {
        "template": "./node_modules/docdash/",
        "encoding": "utf8",
        "destination": "./doc/",
        "recurse": true,
        "private": true
    },
    "docdash": {
        "collapse": true,
        "search": true,
        "scopeInOutputPath": false,
        "nameInOutputPath": false,
        "versionInOutputPath": false
    }
}

